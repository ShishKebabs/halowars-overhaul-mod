<?xml version="1.0" encoding="us-ascii"?>
<Controls>
	<Mode name="GameScreen">
		<Control name="ButtonA" command="accept" event="start" sound="play_menu_accept_button" />
		<Control name="ButtonB" command="cancel" event="start" sound="play_menu_back_button" />
	</Mode>
	<Mode name="ViewTabs">
		<Control name="StickLeftLeft" command="navLeft" event="start" digital="" threshold="0.9" repeat="" delay="0.5" rate="0.2" sound="play_partysystem_menu_up_down" />
		<Control name="StickLeftRight" command="navRight" event="start" digital="" threshold="0.9" repeat="" delay="0.5" rate="0.2" sound="play_partysystem_menu_up_down" />
		<Control name="DpadLeft" command="navLeft" event="start" repeat="" delay="0.5" rate="0.2" sound="play_partysystem_menu_up_down" />
		<Control name="DpadRight" command="navRight" event="start" repeat="" delay="0.5" rate="0.2" sound="play_partysystem_menu_up_down" />
		<Control name="StickLeftUp" command="navUp" event="start" digital="" threshold="0.9" repeat="" delay="0.5" rate="0.2" sound="play_partysystem_menu_up_down" />
		<Control name="StickLeftDown" command="navDown" event="start" digital="" threshold="0.9" repeat="" delay="0.5" rate="0.2" sound="play_partysystem_menu_up_down" />
		<Control name="DpadUp" command="navUp" event="start" repeat="" delay="0.5" rate="0.2" sound="play_partysystem_menu_up_down" />
		<Control name="DpadDown" command="navDown" event="start" repeat="" delay="0.5" rate="0.2" sound="play_partysystem_menu_up_down" />
	</Mode>
</Controls>